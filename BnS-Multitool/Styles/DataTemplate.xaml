<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewmodel="clr-namespace:BnS_Multitool.ViewModels"
                    xmlns:view="clr-namespace:BnS_Multitool.View">
    <DataTemplate DataType="{x:Type viewmodel:MainPageViewModel}">
        <view:MainPage/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewmodel:LauncherViewModel}">
        <view:Launcher/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewmodel:PluginsViewModel}">
        <view:Plugins/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewmodel:ModsViewModel}">
        <view:ModsView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewmodel:SyncViewModel}">
        <view:SyncView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewmodel:PatchesViewModel}">
        <view:PatchesView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewmodel:EffectsViewModel}">
        <view:EffectsView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewmodel:ExtendedOptionsViewModel}">
        <view:ExtendedOptionsView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewmodel:QolViewModel}">
        <view:QolView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewmodel:UpdaterViewModel}">
        <view:UpdaterView/>
    </DataTemplate>
</ResourceDictionary>